<?xml version="1.0" ?>
<obstacles>
   <defines>
    <const name='base_height' value='0.015' />
    <const name='cell_width' value='0.180' />  
    <const name='cell_depth' value='0.120' /> 
    <const name='wall_height' value='0.120' />
    <const name='wall_thickness'  value='0.010' />
    <const name='console_height' value='0.04' />
    <const name='console_width' value='0.12' />  
    <const name='console_depth' value='0.08' /> 
  </defines>

  <cuboid>
    <ID value='base'/>
    <size x='2*cell_depth' y='2*cell_width' z='base_height'/>
    <pos x='0' y='0' z='base_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='64' g='64' b='64'/>
  </cuboid>

  <cuboid>
    <ID value='bottom_wall'/>
    <size x='2*cell_depth' y='wall_thickness' z='wall_height'/>
    <pos x='0' y='-cell_width + wall_thickness/2' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='middle_wall_x'/>
    <size x='2*cell_depth' y='wall_thickness' z='wall_height'/>
    <pos x='0' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='top_wall'/>
    <size x='2*cell_depth' y='wall_thickness' z='wall_height'/>
    <pos x='0' y='cell_width - wall_thickness/2' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='middle_wall_y'/>
    <size x='wall_thickness' y='2*cell_width' z='wall_height'/>
    <pos x='0' y='0' z='base_height + wall_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='240' g='240' b='240'/>
  </cuboid>

  <cuboid>
    <ID value='ceiling'/>
    <size x='2*cell_depth' y='2*cell_width' z='wall_thickness'/>
    <pos x='0' y='0' z='base_height + wall_height + wall_thickness/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='128' g='128' b='128'/>
  </cuboid>

  <cuboid>
    <ID value='console'/>
    <size x='2*cell_depth' y='2*cell_width' z='console_height'/>
    <pos x='0' y='0' z='base_height + wall_height + wall_thickness + console_height/2'/>
    <rot_deg x='0' y='0' z='0'/>
    <color_rgb r='200' g='200' b='200'/>
  </cuboid>

  <sphere>
    <ID value='_LED1'/>
    <radius value='0.005'/>
    <pos x='-cell_depth' y='cell_width/2' z='base_height + wall_height + wall_thickness + console_height/2'/>
    <color_rgb r='255' g='0' b='0'/>
  </sphere>

  <sphere>
    <ID value='_LED2'/>
    <radius value='0.005'/>
    <pos x='cell_depth' y='cell_width/2' z='base_height + wall_height + wall_thickness + console_height/2'/>
    <color_rgb r='255' g='0' b='0'/>
  </sphere>

  <sphere>
    <ID value='_LED3'/>
    <radius value='0.005'/>
    <pos x='-cell_depth' y='-cell_width/2' z='base_height + wall_height + wall_thickness + console_height/2'/>
    <color_rgb r='255' g='0' b='0'/>
  </sphere>

  <sphere>
    <ID value='_LED4'/>
    <radius value='0.005'/>
    <pos x='cell_depth' y='-cell_width/2' z='base_height + wall_height + wall_thickness + console_height/2'/>
    <color_rgb r='255' g='0' b='0'/>
  </sphere>

</obstacles>
